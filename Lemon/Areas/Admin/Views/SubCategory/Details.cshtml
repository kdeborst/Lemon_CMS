@model Lemon.Models.SubCategory
@using Lemon.Extensions

@{
    ViewData["Title"] = "Subcategorie Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="border white-background">
    <h2 class="text-warning">Details Subcategorie</h2>
    <br />
    <div class="col-8">
        <fieldset disabled>
            <div class="form-group row">
                <div class="col-3">
                    <label class="custom-label" asp-for="Name"></label>
                </div>
                <div class="col-8 ">
                    <input asp-for="Name" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-3">
                    <label class="custom-label" asp-for="Category.Name"></label>
                </div>
                <div class="col-8 ">
                    <input class="form-control" asp-for="Category.Name" />
                </div>
            </div>
        </fieldset>
        <div class="form-group row">
            <div class="col-8 offset-3">
                <partial name="_AlterDetailsOrBackButtonPartial" model="Model.Id" />
            </div>
        </div>
    </div>
</div>